    INNER JOIN [dbo].[DimTiempoST] OD ON EV.fecha_pedido = OD.fecha_pedido
    INNER JOIN [dbo].[DimClienteST] DC ON DC.ID_cliente = EV.ID_cliente
    INNER JOIN [dbo].[DimProductoST] PD ON PD.ID_producto = DV.ID_producto
